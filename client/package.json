{
  "name": "cetti",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "npm-run-all postinstall:*",
    "postinstall:ts_base_config": "cp tsconfig.base.json tsconfig.json",
    "postinstall:tsdm": "tsdm rewire",
    "postinstall:tsconfig": "tsconfig -i 2",
    "start": "npm run dev",
    "lint": "npm run lint:typescript; a=$?; npm run lint:scss; exit $(($a + $?))",
    "lint:typescript": "tslint --rules-dir tools/lint-rules `find app -name '*.ts' -o -name '*.tsx'`",
    "lint:scss": "! (sass-lint app/**/*.scss --ignore 'app/style/mdl.global.scss, app/style/react-mdl-layout.scss' --verbose  | grep '^' )",
    "build": "rm -rf dist && NODE_ENV=production webpack",
    "build_dev": "webpack",
    "build_typescript": "tsc",
    "serve_build": "(cd dist && python -m SimpleHTTPServer)",
    "dev": "webpack-dev-server --hot --inline --port 3000",
    "clean": "rm -rf .tmp"
  },
  "dependencies": {
    "history": "^1.17.1",
    "material-design-lite": "^1.0.6",
    "nedb-persist": "^0.1.0",
    "ramda": "^0.18.0",
    "react": "^0.14.2",
    "react-dom": "^0.14.2",
    "react-mdl": "1.0.2",
    "react-router": "^1.0.2",
    "redux": "^3.3.1",
    "redux-decorated": "0.2.0-dev.2",
    "redux-persist": "^1.5.2",
    "redux-websocket": "0.2.0-dev.1",
    "whatwg-fetch": "^0.10.1"
  },
  "devDependencies": {
    "babel": "^6.0.15",
    "babel-cli": "^6.1.1",
    "babel-core": "^6.0.20",
    "babel-loader": "^6.0.1",
    "babel-plugin-jsx-tagclass": "^0.1.2",
    "babel-polyfill": "^6.2.0",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.1.18",
    "babel-preset-stage-1": "^6.0.15",
    "babel-runtime": "^6.0.14",
    "css-loader": "^0.23.0",
    "file-loader": "^0.8.4",
    "node-sass": "^3.4.2",
    "npm-run-all": "^1.2.12",
    "react-hot-loader": "^1.3.0",
    "redux-devtools": "^2.1.5",
    "resolve-url-loader": "^1.4.2",
    "retyped-node-tsd-ambient": "^1.5.3-0",
    "retyped-react-router-tsd-ambient": "^1.0.0-0",
    "retyped-react-tsd-ambient": "^0.14.0-1",
    "retyped-redux-tsd-ambient": "^1.0.0-0",
    "retyped-websocket-tsd-ambient": "0.0.0-0",
    "sass-lint": "^1.3.3",
    "sass-loader": "^3.1.2",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.0",
    "ts-loader": "^0.6.0",
    "tsconfig-glob": "^0.2.1",
    "tsdm": "0.1.0-3",
    "tslint": "^2.5.1",
    "typescript": "^1.7.5",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.1"
  },
  "homepage": "https://github.com/drager/cetti#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/drager/cetti.git"
  },
  "bugs": {
    "url": "https://github.com/drager/cetti/issues"
  }
}
